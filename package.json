{
  "name": "@znemz/eslint-plugin-nem",
  "version": "0.0.1",
  "description": "eslint custom rules",
  "main": "./lib/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prelint": "editorconfig-tools check * tests/*",
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./tests/**/*spec.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || yarn test)",
    "pretest": "yarn lint",
    "test": "yarn tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nmccready/eslint-plugin-nem.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide"
  ],
  "author": "Nicholas McCready",
  "originlAuthors": [
    "AirBNB"
  ],
  "dependencies": {
    "camel-case": "^3.0.0",
    "lodash": "^4.17.11",
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-tape-runner": "^2.0.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "generator-eslint": "^1.2.0",
    "in-publish": "^2.0.0",
    "react": ">= 0.13.0",
    "safe-publish-latest": "^1.1.1",
    "tape": "^4.8.0",
    "yo": "^2.0.5"
  },
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "lib"
  ]
}
